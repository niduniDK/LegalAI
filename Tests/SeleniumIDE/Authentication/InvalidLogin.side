    }, {
      "id": "7",
      "comment": "Click submit login",
      "command": "click",
      "target": "css=[data-testid=\"login-submit\"]",
      "targets": [
        ["css=[data-testid=\"login-submit\"]", "css:data-testid"],
        ["css=button[type=\"submit\"]", "css:type"]
      ],
      "value": ""
    }, {
      "id": "8",
      "comment": "Wait for error message",
      "command": "waitForElementVisible",
      "target": "css=[data-testid=\"error-message\"]",
      "targets": [],
      "value": "3000"
    }, {
      "id": "9",
      "comment": "Verify error message is displayed",
      "command": "assertElementPresent",
      "target": "css=[data-testid=\"error-message\"]",
      "targets": [],
      "value": ""
    }, {
      "id": "10",
      "comment": "Verify still on login page",
      "command": "assertElementPresent",
      "target": "css=[data-testid=\"login-submit\"]",
      "targets": [],
      "value": ""
    }]
  }],
  "suites": [{
    "id": "invalid-login-suite",
    "name": "Invalid Login Test Suite",
    "persistSession": false,
    "parallel": false,
    "timeout": 300,
    "tests": ["invalid-login-wrong-password", "invalid-login-wrong-email"]
  }],
  "urls": ["http://localhost:3000"],
  "plugins": []
}