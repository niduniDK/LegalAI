  "name": "LegalAI Complete Test Suite",
  "url": "http://localhost:3000",
  "tests": [
    {
      "id": "test-valid-login",
      "name": "Valid Login Test",
      "commands": [
        {
          "id": "cmd-0",
          "comment": "Store login status variable",
          "command": "store",
          "target": "false",
          "targets": [],
          "value": "isLoggedIn"
        },
        {
          "id": "cmd-1",
          "comment": "Navigate to login page",
          "command": "open",
          "target": "/login",
          "targets": [],
          "value": ""
        },
        {
          "id": "cmd-2",
          "comment": "Enter valid email",
          "command": "type",
          "target": "id=email",
          "targets": [
            ["id=email", "id"],
            ["name=email", "name"],
            ["css=input[type=\"email\"]", "css:finder"]
          ],
          "value": "test@example.com"
        },
        {
          "id": "cmd-3",
          "comment": "Enter valid password",
          "command": "type",
          "target": "id=password",
          "targets": [
            ["id=password", "id"],
            ["name=password", "name"],
            ["css=input[type=\"password\"]", "css:finder"]
          ],
          "value": "Test123!@#"
        },
        {
          "id": "cmd-4",
          "comment": "Click login button",
          "command": "click",
          "target": "css=button[type=\"submit\"]",
          "targets": [
            ["css=button[type=\"submit\"]", "css:finder"],
            ["xpath=//button[contains(text(),'Sign In')]", "xpath:text"],
            ["css=button.w-full", "css:finder"]
          ],
          "value": ""
        },
        {
          "id": "cmd-4a",
          "comment": "Wait for redirect after login",
          "command": "waitForElementNotPresent",
          "target": "xpath=//h2[contains(text(),'Sign in to LegalAI')]",
          "targets": [
            ["xpath=//h2[contains(text(),'Sign in to LegalAI')]", "xpath:text"]
          ],
          "value": "5000"
        },
        {
          "id": "cmd-5",
          "comment": "Verify successful login (redirected to home)",
          "command": "waitForElementPresent",
          "target": "xpath=//h1[text()='Break Legal jargon here.']",
          "targets": [
            ["xpath=//h1[text()='Break Legal jargon here.']", "xpath:text"],
            ["xpath=//h1[contains(text(),'Break Legal jargon')]", "xpath:text"],
            ["css=h1", "css:finder"]
          ],
          "value": "10000"
        },
        {
          "id": "cmd-5a",
          "comment": "Verify sidebar is present (confirms authentication)",
          "command": "waitForElementPresent",
          "target": "xpath=//span[text()='LegalAI']",
          "targets": [
            ["xpath=//span[text()='LegalAI']", "xpath:text"],
            ["css=span.text-sm.font-semibold", "css:finder"]
          ],
          "value": "5000"
        },
        {
          "id": "cmd-6",
          "comment": "Set login status to true",
          "command": "store",
          "target": "true",
          "targets": [],
          "value": "isLoggedIn"
        }
      ]
    },
