    {
      "id": "test-send-chat-message",
      "name": "Send Chat Message Test",
      "commands": [
        {
          "id": "cmd-20",
          "comment": "Navigate to home page",
          "command": "open",
          "target": "/",
          "targets": [],
          "value": ""
        },
        {
          "id": "cmd-21",
          "comment": "Wait for chat interface to load",
          "command": "waitForElementPresent",
          "target": "css=input[placeholder*=\"Ask anything\"]",
          "targets": [
            ["css=input[placeholder*=\"Ask anything\"]", "css:finder"],
            ["css=.w-full.h-14", "css:finder"]
          ],
          "value": "3000"
        },
        {
          "id": "cmd-22",
          "comment": "Enter chat message",
          "command": "type",
          "target": "css=input[placeholder*=\"Ask anything\"]",
          "targets": [
            ["css=input[placeholder*=\"Ask anything\"]", "css:finder"],
            ["css=.w-full.h-14", "css:finder"]
          ],
          "value": "What are the legal requirements for starting a business in Sri Lanka?"
        },
        {
          "id": "cmd-23",
          "comment": "Click send button (arrow icon)",
          "command": "click",
          "target": "css=button[class*=\"absolute right-2\"]",
          "targets": [
            ["css=button[class*=\"absolute right-2\"]", "css:finder"],
            ["css=button.h-10.w-10", "css:finder"],
            ["xpath=//button[contains(@class,'absolute right-2')]", "xpath:class"]
          ],
          "value": ""
        },
        {
          "id": "cmd-24",
          "comment": "Wait for AI response",
          "command": "waitForElementPresent",
          "target": "css=.bg-muted.px-4.py-3",
          "targets": [
            ["css=.bg-muted.px-4.py-3", "css:finder"],
            ["css=div[class*=\"bg-muted\"]", "css:finder"],
            ["xpath=//div[contains(@class,'bg-muted')]", "xpath:class"]
          ],
          "value": "10000"
        },
        {
          "id": "cmd-25",
          "comment": "Verify response received",
          "command": "assertElementPresent",
          "target": "css=.bg-muted.px-4.py-3",
          "targets": [
            ["css=.bg-muted.px-4.py-3", "css:finder"],
            ["css=div[class*=\"bg-muted\"]", "css:finder"],
            ["xpath=//div[contains(@class,'bg-muted')]", "xpath:class"]
          ],
          "value": ""
        }
      ]
