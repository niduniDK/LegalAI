   "source": [
    "import os\n",
    "from pdf2image import convert_from_path\n",
    "import torch\n",
    "from surya.model.detection.model import load_model as load_det_model\n",
    "from surya.model.recognition.model import load_model as load_rec_model\n",
    "from surya.model.recognition.processor import load_processor as load_rec_processor\n",
    "from surya.ocr import run_ocr\n",
    "from transformers import SegformerImageProcessor\n",
    "import pandas as pd\n",
    "\n",
    "# -------------------------------\n",
    "# paths\n",
    "# -------------------------------\n",
    "PDF_PATH = \"/kaggle/input/constitution-si/constitution-si.pdf\"  \n",
    "SAVE_PATH = \"/kaggle/working/constitution_si.csv\"\n",
    "\n",
    "# -------------------------------\n",
    "# initialize Surya OCR models (CPU)\n",
    "# -------------------------------\n",
    "print(\"Initializing Surya OCR models (CPU)...\")\n",
    "device = torch.device(\"cpu\")\n",
    "det_model = load_det_model().to(device)\n",
    "rec_model = load_rec_model().to(device)\n",
    "rec_processor = load_rec_processor()\n",
    "try:\n",
    "    det_processor = SegformerImageProcessor.from_pretrained(\"vikp/surya_det3\")\n",
    "except Exception:\n",
    "    det_processor = None\n",
    "print(\"✅ Surya models ready on CPU!\")\n",
    "\n",
    "# -------------------------------\n",
    "# OCR functions\n",
    "# -------------------------------\n",
    "def ocr_page_surya(img, langs=[\"si\"]):\n",
    "    \"\"\"Run Surya OCR on a single page image\"\"\"\n",
    "    preds = run_ocr([img], [langs], det_model, det_processor, rec_model, rec_processor)\n",
    "    if preds and preds[0].text_lines:\n",
    "        return \" \".join([line.text for line in preds[0].text_lines])\n",
    "    return \"\"\n",
    "\n",
    "\n",
    "def ocr_pdf_local(pdf_path, langs=[\"si\"]):\n",
    "    \"\"\"OCR a local PDF page by page using Surya OCR\"\"\"\n",
    "    images = convert_from_path(pdf_path, dpi=200)\n",
    "    full_text = \"\"\n",
    "    for i, img in enumerate(images):\n",
    "        page_text = ocr_page_surya(img, langs=langs)\n",
    "        full_text += page_text + \"\\n\\n\"\n",
    "    return full_text.strip()\n",
    "\n",
    "\n",
    "# -------------------------------\n",
    "# main\n",
    "# -------------------------------\n",
    "if __name__ == \"__main__\":\n",
    "    if not os.path.exists(PDF_PATH):\n",
    "        print(f\"❌ PDF file does not exist: {PDF_PATH}\")\n",
    "    else:\n",
    "        text = ocr_pdf_local(PDF_PATH, langs=[\"si\"])\n",
    "        print(\"✅ OCR complete, total characters:\", text[:500])\n",
    "\n",
    "        # save results to CSV\n",
    "        df = pd.DataFrame([{\n",
    "            \"name\": os.path.basename(PDF_PATH),\n",
    "            \"text\": text,\n",
    "            \"character_count\": len(text)\n",
    "        }])\n",
    "        df.to_csv(SAVE_PATH, index=False, encoding=\"utf-8-sig\")\n",
    "        print(\"✅ Results saved to\", SAVE_PATH)\n"
   ]
  },
